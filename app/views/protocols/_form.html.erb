<%= form_for @protocol, html: {class: 'ui form'} do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <%= f.submit "Cadastrar Protocolo", class: 'ui button blue' %>
<% end %>
